{
  "name": "device-agnostic-ui",
  "version": "6.0.0",
  "description": "Device agnostic styles, components and hooks for Next.js.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/device-agnostic-ui",
  "homepage": "https://deviceagnosticui.com",
  "bugs": "https://github.com/jaydenseric/device-agnostic-ui/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "device",
    "agnostic",
    "ui",
    "react",
    "styled-jsx",
    "next"
  ],
  "files": [
    "public"
  ],
  "main": "public",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./public/index.mjs",
      "require": "./public/index.js"
    },
    "./public/": "./public/",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^10.17.0 || ^12.0.0 || >= 13.7.0"
  },
  "browserslist": "Node 10.17 - 11 and Node < 11, Node 12 - 13 and Node < 13, Node >= 13.7, > 0.5%, not OperaMini all, not dead",
  "peerDependencies": {
    "next": "9.0.4 - 10",
    "react": "16.14.0 - 17",
    "react-dom": "16.14.0 - 17",
    "styled-jsx": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "class-name-prop": "^2.0.0",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "coverage-node": "^4.0.0",
    "eslint": "^7.16.0",
    "eslint-config-env": "^15.0.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next": "^10.0.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-jsx": "^3.3.2",
    "test-director": "^5.0.0"
  },
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:babel && npm run prepare:prettier",
    "prepare:clean": "rm -rf public",
    "prepare:babel": "babel src -d . --keep-file-extension",
    "prepare:prettier": "prettier --write public readme.md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api",
    "test:eslint": "eslint --ext mjs,js .",
    "test:prettier": "prettier -c .",
    "test:api": "coverage-node test",
    "prepublishOnly": "npm test"
  }
}
