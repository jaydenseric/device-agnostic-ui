{
  "name": "device-agnostic-ui",
  "version": "5.1.1",
  "description": "Device agnostic styles, components and hooks for Next.js.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/device-agnostic-ui",
  "homepage": "https://deviceagnosticui.com",
  "bugs": "https://github.com/jaydenseric/device-agnostic-ui/issues",
  "keywords": [
    "device",
    "agnostic",
    "ui",
    "react",
    "styled-jsx",
    "next"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/cjs",
  "module": "lib/esm",
  "sideEffects": false,
  "engines": {
    "node": "10 - 12 || >= 13.7"
  },
  "browserslist": "Node 10 - 12, Node >= 13.7, > 0.5%, not OperaMini all, not dead",
  "peerDependencies": {
    "next": "^9.0.4",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "styled-jsx": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "class-name-prop": "^2.0.0",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@size-limit/preset-small-lib": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "eslint": "^7.0.0",
    "eslint-config-env": "^13.0.6",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "next": "^9.3.6",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "size-limit": "^4.5.0",
    "styled-jsx": "^3.2.5",
    "test-director": "^4.0.1"
  },
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:esm && npm run prepare:cjs && npm run prepare:prettier",
    "prepare:clean": "rm -rf lib",
    "prepare:esm": "BABEL_ESM=1 babel src -d lib/esm",
    "prepare:cjs": "babel src -d lib/cjs",
    "prepare:prettier": "prettier 'lib/**/*.{mjs,js}' readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api && npm run test:size",
    "test:eslint": "eslint . --ext mjs,js",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "test:api": "node test",
    "test:size": "size-limit",
    "prepublishOnly": "npm test"
  }
}
